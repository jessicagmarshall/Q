{
  "manifest_version": 2,
  "name": "Qrome Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "Q.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
          "tabs"
  ]
}
